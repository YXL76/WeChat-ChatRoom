  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "module": "esnext",
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "lib": ["esnext", "dom", "dom.iterable"]
  },
  "exclude": ["build", "dist", "config", "node_modules"]
}
